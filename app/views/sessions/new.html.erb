<h4>Log In</h4>

<section class='login-form'>
  <%= form_for :session do |f| %>
    <div>
      <p id='label'><%= f.label :email %></p>
      <p id='field'><%= f.email_field :email %></p>
    </div>
    <div>
      <p id='label'><%= f.label :password %></p>
      <p id='field'><%= f.password_field :password %></p>
    </div>
    <p id='submit'><%= f.submit 'Log In' %></p>
  <% end %>

  <%= link_to image_tag('btn_google_signin_dark_normal_web.png'), '/auth/google_oauth2', { class: 'google_button_image' } %>
</section>
